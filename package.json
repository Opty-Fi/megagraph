{
  "name": "megagraph",
  "version": "1.0.0",
  "description": "This package contains subgraph DeFi protocols that opty.fi's earn protocol integrates natively",
  "author": "Opty-Fi <info@opty.fi>",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Opty-Fi/megagraph"
  },
  "scripts": {
    "lint": "yarn run lint:ts",
    "lint:ts": "eslint --config ./.eslintrc.yaml --ignore-path ./.eslintignore --ext .js,.ts",
    "prettier": "prettier --config ./.prettierrc.yaml --write \"**/*.{js,json,md,ts}\"",
    "prettier:check": "prettier --check --config ./.prettierrc.yaml \"**/*.{js,json,md,ts}\"",
    "mustache-yaml": "./node_modules/.bin/dotenv -- yarn mustache-yaml-replace",
    "mustache-yaml-replace": "mustache ./src/${ADAPTER}/config/${CONFIG}.json subgraph.template.yaml > subgraph.yaml",
    "postmustache-yaml-replace": "cp src/${ADAPTER}/schema.graphql schema.graphql",
    "codegen": "yarn mustache-yaml && rm -rf build generated && ./node_modules/@graphprotocol/graph-cli/bin/graph codegen",
    "postcodegen": "./node_modules/.bin/dotenv -- ./script/add-symlinks.sh",
    "auth": "./node_modules/.bin/dotenv -- yarn auth-replace",
    "auth-replace": "./node_modules/@graphprotocol/graph-cli/bin/graph auth --studio ${STUDIO_DEPLOY_KEY}",
    "deploy": "./node_modules/.bin/dotenv -- yarn deploy-replace",
    "deploy-replace": "./node_modules/@graphprotocol/graph-cli/bin/graph deploy --studio --deploy-key ${STUDIO_DEPLOY_KEY} --version-label ${STUDIO_VERSION} ${STUDIO_SLUG}",
    "setup:ethereum": "./node_modules/.bin/dotenv -- ./script/setup-ethereum.sh",
    "setup:polygon": "./node_modules/.bin/dotenv -- ./script/setup-polygon.sh",
    "setup:avalanche": "./node_modules/.bin/dotenv -- ./script/setup-avalanche.sh",
    "setup:fantom": "./node_modules/.bin/dotenv -- ./script/setup-fantom.sh",
    "setup:all": "yarn setup:ethereum && yarn setup:polygon && yarn setup:avalanche && yarn setup:fantom",
    "setup": "yarn codegen && yarn auth && yarn deploy",
    "mustache-yaml:local": "./node_modules/.bin/dotenv -- yarn mustache-yaml:local-replace",
    "mustache-yaml:local-replace": "./node_modules/node-jq/bin/jq 'del(.contracts | .[] | .instances | .[] | .startBlock)' ./src/${ADAPTER}/config/${CONFIG}.json > ./src/${ADAPTER}/config/${CONFIG}-dev.json && mustache ./src/${ADAPTER}/config/${CONFIG}-dev.json subgraph.template.yaml > subgraph.yaml",
    "postmustache-yaml:local-replace": "cp src/${ADAPTER}/schema.graphql schema.graphql",
    "codegen:local": "yarn mustache-yaml:local && rm -rf build generated && ./node_modules/@graphprotocol/graph-cli/bin/graph codegen",
    "postcodegen:local": "./node_modules/.bin/dotenv -- ./script/add-symlinks.sh",
    "build:dev": "yarn codegen:local && ./node_modules/@graphprotocol/graph-cli/bin/graph build",
    "create:local": "./node_modules/.bin/dotenv -- yarn create:local-replace",
    "create:local-replace": "./node_modules/@graphprotocol/graph-cli/bin/graph create --studio --node http://localhost:8020/ ${STUDIO_DEPLOY_KEY}",
    "deploy:local": "./node_modules/.bin/dotenv -- yarn deploy:local-replace",
    "deploy:local-replace": "./node_modules/@graphprotocol/graph-cli/bin/graph deploy --studio --deploy-key ${STUDIO_DEPLOY_KEY} --version-label ${STUDIO_VERSION} --node http://localhost:8020/ --ipfs http://localhost:5001/ ${STUDIO_SLUG}",
    "setup:local": "yarn codegen:local && yarn create:local && yarn deploy:local"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.30.4",
    "@graphprotocol/graph-ts": "^0.27.0",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^5.1.0",
    "mustache": "^4.2.0",
    "node-jq": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.4.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.7.4"
  }
}
